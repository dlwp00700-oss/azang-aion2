{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "routes": [
    {
      "src": "/sitemap.xml",
      "headers": {
        "Content-Type": "application/xml"
      },
      "dest": "/sitemap.xml"
    },
    {
      "src": "/robots.txt",
      "headers": {
        "Content-Type": "text/plain"
      },
      "dest": "/robots.txt"
    }
  ]
}
